{
  "Convention": "InPlaceOverwrite",
  "LinkFormat": "GitHub",
  "UrlPrefix": "",
  "ReadOnly": false,
  "OmitSnippetLinks": false,
  "ExcludeDirectories": [
    "node_modules",
    "bin",
    "obj",
    ".git",
    "Generated",
    "todos",
    "release-notes"
  ],
  "TreatMissingAsWarning": true,
  "ExcludeMarkdownDirectories": [
    "docs/todos",
    ".claude"
  ],
  "DocumentExtensions": [
    "md"
  ]
}
